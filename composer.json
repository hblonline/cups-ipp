{
    "name": "hblonline/cups-ipp",
    "description": "CUPS Implementation of IPP - PHP Client API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fixami Netherlands B.V.",
            "email": "ict@fixami.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "php-http/socket-client": "dev-master#93df2cc24c1b59a40ab7fc9d416e32964cdcc839",
        "php-http/client-common": "*",
        "php-http/message": "*",
        "psr/http-client": "^1.0.1",
        "guzzlehttp/psr7": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "atoum/atoum": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:php-http/socket-client.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "wapmorgan/file-type-detector": "Detects file type by filename or content and generates correct mimetype."
    },
    "autoload": {
        "psr-4": {
            "Smalot\\Cups\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smalot\\Cups\\Tests\\Units\\": "tests/"
        }
    }
}
